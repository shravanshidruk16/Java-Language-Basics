import java.util.*;
public class Main{
    public static void swap(int n1, int n2){
        int temp=n1;
        n1=n2;
        n2=temp;
        System.out.println("a= "+n1+", b= "+n2);
    }
    public static void main(String [] args){
        int a=3,b=4;
        swap(a, b);
        // after the function call ends or the function executes then the temporary or copied values are deleted and the original values are used again
        System.out.println("a= "+a+", b= "+b);

    }
}
